<view style=' background-color:  white;height:100%;'>
   <view class='userinfo'>
    <!-- 头像-->
     <view style='width: 30%;height: 30%;border-radius: 50%;margin:3%;overflow:hidden;' bindtap='changeToUserinfo'>
      <open-data type="userAvatarUrl" ></open-data>
    </view>
    <!-- 昵称--> 
     <view style='width: 75%;height: 25%;margin:10%;'>
      <open-data type="userNickName" ></open-data>
    </view>
    </view>  

  <!--功能键-->
<view class='function'>
 <view>
   <button style='height:20%;margin:5%;' bindtap='changeToRecievingloc'>常用收件地点 </button>
 </view>
 <view class='record'>
   <button style='height:20%;margin:5%;' bindtap='changeTohistory'>预约记录</button>
 </view>
</view>
</view>

