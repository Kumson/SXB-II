/* miniprogram/pages/frontpage/frontpage.wxss */
page {
  background-color: rgb(0, 0, 0);
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  width: 100%;
}
.bg{
  width: 100%;
  height:100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;

}
.plus{
  margin-top: 3%;
  height: 30px;
  border-radius: 5%;
  display:flex; 
  flex-direction: column;
  text-align: center;
  color: white;
  font-size: 75%;
  
}
.plus1{
  height: 120rpx;
  width: 20%;
  margin-left: 40%;
}
.order{
  background-color:rgba(0, 0, 0, 0.116);
  width: 100%;
  border-radius: 5%;
  margin-top:3%;
}

.holeBox{
  display: flex;
  flex-direction: row;
  margin:3%; 
}
.ltefBox{
  width: 90%;
}
.leftText{
  color: #d2d5da; 
  font-size: 80%;
}
.stateText{

  width: 100%;
  color: white;
  font-size: 120%;
  margin-left:5%;
}
.rightBox{
  display: flex;
  flex-direction: column;
  width: 20%;
  text-align: center;
  font-size: 90%;
  border-radius: 3%;
  color: white;
}
.rightBoxYes{
  
  background-color: rgba(0, 255, 55, 0.349);
}
.rightBoxNo{
  
  background-color: rgba(255, 8, 0, 0.349)
}